{
  "name": "telegram-bot",
  "private": true,
  "version": "1.0.0",
  "description": "Bot that will add the IMDB score into our NAS chat group",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "node --env-file=.env --watch --trace-warnings src/index.js",
    "dev-proto": "node --env-file=.env --watch --trace-warnings mProtoBot.js",
    "start": "node --env-file=.env index.js",
    "start-proto": "node --env-file=.env mProtoBot.js",
    "build": "swc src --out-dir dist/src",
    "build:ci": "tsc && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jorge Martin",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.2",
    "mtproto-core": "^6.3.0",
    "node-telegram-bot-api": "^0.64.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101"
  }
}
